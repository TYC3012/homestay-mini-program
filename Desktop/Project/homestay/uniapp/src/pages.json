{
	"easycom": {
		"^tn-(.*)": "@/tuniao-ui/components/tn-$1/tn-$1.vue"
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index",
			"style": {
				"enablePullDownRefresh": true,
				"mp-weixin": {
					"disableScroll": false,
					"lazyCodeLoading": "requiredComponents"
				},
				"app-plus": {
					"bounce": "none"
				},
				"mp-alipay": {
					"allowsBounceVertical": "NO"
				},
				"mp-baidu": {
					"disableScroll": true
				}
			}
		}
	],
	"subPackages": [{
	  "root": "homePages",
	  "pages": [
		  {
		  "path": "listDetail",
		  "style": {
			  "navigationBarTitleText": "房源详情页",
			  "enablePullDownRefresh": false
		  }
	  	},
		  {
			  "path": "confirmOrder",
			  "style": {
				  "navigationBarTitleText": "订单确认页",
				  "enablePullDownRefresh": false
			  }
		  },
		  {
			  "path": "orderList",
			  "style": {
				  "navigationBarTitleText": "订单列表",
				  "enablePullDownRefresh": false
			  }
		  },
		  {
			  "path": "shopOrderList",
			  "style": {
				  "navigationBarTitleText": "商家订单列表",
				  "enablePullDownRefresh": false
			  }
		  },
		  {
			  "path": "roomList",
			  "style": {
				  "navigationBarTitleText": "房间列表",
				  "enablePullDownRefresh": false
			  }
		  },
		  {
			  "path": "evaluate",
			  "style": {
				  "navigationBarTitleText": "评价界面",
				  "enablePullDownRefresh": false
			  }
		  },
		  {
			  "path": "set",
			  "style": {
				  "navigationBarTitleText": "个人设置",
				  "enablePullDownRefresh": false
			  }
		  },
		  {
			  "path": "storeOwner",
			  "style": {
				  "navigationBarTitleText": "店主注册",
				  "enablePullDownRefresh": false
			  }
		  }
	  ]
	},{
		"root": "myPages",
		"pages": [{
			"path": "login",
			"style": {
				"navigationBarTitleText": "授权登录",
				"enablePullDownRefresh": false
			}
		}]
	}],
	"globalStyle": {
		"navigationStyle": "custom",
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "安心民宿",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	}
}
